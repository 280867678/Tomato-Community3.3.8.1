<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:layout_width="match_parent" android:layout_height="match_parent">
    <FrameLayout android:id="@+id/fl_content_view" android:layout_width="match_parent" android:layout_height="match_parent">
        <com.tomatolive.library.p136ui.view.widget.Html5WebView android:id="@+id/web_view" android:background="#00000000" android:layout_width="match_parent" android:layout_height="match_parent"/>
        <TextView android:textSize="14dp" android:textColor="@color/fq_text_gray" android:gravity="center" android:id="@+id/tv_loading_view" android:background="@color/fq_colorWhite" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="match_parent" android:text="@string/hint_text_dialog_loading_data"/>
        <TextView android:textSize="14dp" android:textColor="@color/fq_text_gray" android:gravity="center" android:id="@+id/tv_loading_fail" android:background="@color/fq_colorWhite" android:visibility="invisible" android:layout_width="match_parent" android:layout_height="match_parent" android:text="@string/fq_data_loading_fail_retry"/>
    </FrameLayout>
</LinearLayout>
