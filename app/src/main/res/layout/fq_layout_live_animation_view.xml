<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.opensource.svgaplayer.SVGAImageView android:id="@+id/sv_anim_big_gift" android:layout_width="match_parent" android:layout_height="match_parent" app:antiAlias="true" app:loopCount="1"/>
    <com.opensource.svgaplayer.SVGAImageView android:id="@+id/sv_anim_car" android:layout_width="match_parent" android:layout_height="128dp" android:layout_above="@+id/sv_anim_enter" app:antiAlias="true" app:loopCount="1"/>
    <com.opensource.svgaplayer.SVGAImageView android:id="@+id/sv_anim_enter" android:layout_width="match_parent" android:layout_height="60dp" android:layout_below="@+id/ll_left_anim_bg" app:loopCount="1"/>
    <RelativeLayout android:id="@+id/ll_left_anim_bg" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_centerVertical="true">
        <LinearLayout android:orientation="vertical" android:id="@+id/ll_left_anim_gift" android:layout_width="match_parent" android:layout_height="wrap_content"/>
        <LinearLayout android:orientation="vertical" android:id="@+id/ll_left_anim" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_below="@+id/ll_left_anim_gift"/>
    </RelativeLayout>
</merge>
