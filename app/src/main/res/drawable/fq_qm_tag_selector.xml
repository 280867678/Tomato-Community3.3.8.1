<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/fq_qm_gray_tips"/>
            <corners android:radius="@dimen/fq_qm_shape_radius"/>
        </shape>
    </item>
    <item android:state_selected="true">
        <shape>
            <solid android:color="#ffffdde9"/>
            <corners android:radius="@dimen/fq_qm_shape_radius"/>
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="1dp" android:color="@color/fq_qm_gray_frame"/>
            <solid android:color="@color/fq_colorWhite"/>
            <corners android:radius="@dimen/fq_qm_shape_radius"/>
        </shape>
    </item>
</selector>
